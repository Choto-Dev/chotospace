{
  "name": "@chotospace/utils",
  "description": "Utility for chotospaceStack",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "unbuild",
    "check-types": "tsc --noEmit",
    "lint": "biome check --write ."
  },
  "devDependencies": {
    "@chotospace/typescript-config": "workspace:*",
    "@types/node": "^24.5.2",
    "tsx": "^4.20.6",
    "typescript": "catalog:",
    "unbuild": "^3.6.1"
  },
  "dependencies": {
    "effect": "^3.18.4",
    "env-paths": "^3.0.0",
    "giget": "2.0.0",
    "yaml": "2.8.1",
    "zod": "4.1.11"
  },
  "exports": {
    ".": {
      "default": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./zod": {
      "default": "./dist/lib/zod.mjs",
      "import": "./dist/lib/zod.mjs",
      "require": "./dist/lib/zod.cjs",
      "types": "./dist/lib/zod.d.ts"
    },
    "./effect": {
      "default": "./dist/lib/effect.mjs",
      "import": "./dist/lib/effect.mjs",
      "require": "./dist/lib/effect.cjs",
      "types": "./dist/lib/effect.d.ts"
    }
  }
}
